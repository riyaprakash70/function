#include <stdio.h>

int sum (int n){
    int rem, sum=0,rev=0;
    while (n>0){
        rem=n%10;
        sum=sum+rem;
        n=n/10;
    }
    return sum; 
}
int main(){
    int n;
    printf("enter any number");
    scanf("%d",&n);
    int result =sum(n);
    printf("&d",result);
    return 0;
}
